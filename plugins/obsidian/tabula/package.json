{
  "name": "tabula",
  "version": "1.1.0",
  "description": "CSV viewer and editor for Obsidian",
  "main": "main.js",
  "scripts": {
    "prebuild": "cp ./src/styles.css ./.obsidian/plugins/tabula/; cp ./manifest.json ./.obsidian/plugins/tabula/",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "predev": "npm run prebuild",
    "dev": "node esbuild.config.mjs",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest src/__tests__/services/ src/__tests__/utils/ src/__tests__/components/",
    "test:integration": "jest src/__tests__/integration/",
    "test:ci": "jest --coverage --ci --watchAll=false",
    "lint": "tsc --noEmit",
    "lint:fix": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@codemirror/commands": "^6.8.1",
    "@codemirror/language": "^6.11.1",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.2",
    "original-fs": "^1.2.0",
    "papaparse": "^5.5.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^18.7.18",
    "@types/papaparse": "^5.3.16",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "0.17.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "obsidian": "latest",
    "parallelshell": "^3.0.1",
    "ts-jest": "^29.1.1",
    "tslib": "2.4.0",
    "typescript": "^4.9.5"
  }
}
